NAME = ft_display_file
SRC = display_file.c
WGCC = gcc -Wextra -Wall -Werror
OBJECTS =

all: $(NAME)

$(NAME):
	$(WGCC) -o $(NAME) $(SRC)

clean:
	rm -f $(OBJECTS)

fclean: clean
	rm -f $(NAME)
